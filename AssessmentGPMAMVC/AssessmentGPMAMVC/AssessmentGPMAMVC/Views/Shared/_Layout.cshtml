<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Aptitude Test - FREE Online Practice Aptitude Tests</title>
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description"
          content="Practice Aptitude Tests Online | Over 400,000 people have improved their test performance using our tests. Practice the tests employers use. Try one now!" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.2/css/all.min.css" integrity="sha512-Evv84Mr4kqVGRNSgIGL/F/aIDqQb7xQ2vcrdIwxfjThSH8CSR7PBEakCr51Ck+w+/U6swU2Im1vVX0SVk9ABhg==" crossorigin="anonymous" referrerpolicy="no-referrer" />

    <link href="~/font-awesome/css/all.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/ad2022-custom.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/iframe-lightbox.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    @await RenderSectionAsync("css",false)
</head>
<body>
    <header id="header">
        <div class="container">
            <div class="logo">
                <a asp-action="Index" asp-controller="Home">
                    <img src="~/img/GPMA.webp"
                         width="140"
                         height="30"
                         alt="AssessmentDay, Practice Test Experts" />
                </a>
            </div>
            <button id="nav-opener" type="button" class="nav-opener">
                <span>Open menu</span>
            </button>
            <div class="nav-slide">
                <div class="search-holder dropdown-holder">
                    <button id="addSearchInput"
                            type="button"
                            class="search-opener dropdown-opener">
                    </button>
                </div>
                <div class="nav-buttons">
                    @if (User.Identity.IsAuthenticated)
                    {
                        <a asp-controller="Auth" asp-action="Signout"
                        class="btn btn-outline-dark rounded"
                           aria-label="login to your account"
                           id="log-in">
                            <span>تسجيل الخروج </span><i class="fa-solid fa-right-long"></i>
                           
                        </a>
                      

                    }
                    else
                    {
                        <a asp-action="Register" asp-controller="Auth"
                           class="btn btn-outline-dark rounded"
                           aria-label="login to your account"
                           id="log-in">
                            <span>التسجيل </span><i class="fa-solid fa-right-long"></i>
                        </a>
                        <a asp-action="LogIn" asp-controller="Auth"
                           id="start-practising"
                           class="btn btn-primary rounded">
                            <span>تسجيل الدخول </span><i class="fa-solid fa-right-long"></i>
                        </a>

                    }
                    <a asp-action="Index" asp-controller="Questions"
                       id="start-practising"
                       class="btn btn-danger rounded">
                        <span>AdminDashboard</span><i class="fa-solid fa-right-long"></i>
                    </a>

                </div>
            </div>
        </div>
    </header>

            @RenderBody()


  
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/cookie-consent.js" asp-append-version="true"></script>
    <script src="~/js/iframe-lightbox.js" asp-append-version="true"></script>
    <script src="~/js/app.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
